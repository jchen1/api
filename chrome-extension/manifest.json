{
  "name": "Track Tabs",
  "version": "0.1",
  "description": "track tabs in api.jeffchen.dev",
  "manifest_version": 2,
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
